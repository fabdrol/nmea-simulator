{
  "name": "nmea-simulator",
  "version": "0.1.1",
  "description": "Simple utility that reads NMEA 0183 lines from a file, sets up a virtual serial port and emits the lines on this serial port at 1 Hz.",
  "main": "./index.js",
  "preferGlobal": true,
  "bin": {
    "nmea-simulator": "./bin/nmea-simulator"
  },
  "repository": {
