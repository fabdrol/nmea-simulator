    "url": "git://github.com/fabdrol/nmea-simulator.git"
  },
  "keywords": [
    "nmea",
    "simulator",
    "serial",
    "serialport",
    "virtual",
    "virtual",
    "serial",
    "port"
  ],
  "author": "Fabian Tollenaar <fabian@starting-point.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabdrol/nmea-simulator/issues"
  },
  "homepage": "https://github.com/fabdrol/nmea-simulator",
  "dependencies": {
    "line-by-line": "^0.1.3",
    "nomnom": "^1.8.1",
    "serialport": "^1.4.6",
    "stream-throttle": "^0.1.3"
  }
}
